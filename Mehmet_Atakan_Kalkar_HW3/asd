The maze has 7 loops
loop 1: 1,9 - 1,10 - 2,10 - 2,9

loop 2: 3,4 - 3,5 - 3,6 - 2,6 - 1,6 - 1,5 - 1,4 - 1,3 - 2,3 - 2,2 - 3,2 - 4,2 - 4,3 - 4,4

loop 3: 4,6 - 5,6 - 5,7 - 6,7 - 6,6 - 6,5 - 5,5 - 4,5

loop 4: 5,6 - 5,7 - 6,7 - 6,6

loop 5: 5,6 - 5,7 - 6,7 - 6,6 - 6,5 - 5,5

loop 6: 8,7 - 7,7 - 7,8 - 8,8

loop 7: 9,7 - 8,7 - 7,7 - 7,8 - 8,8 - 9,8

